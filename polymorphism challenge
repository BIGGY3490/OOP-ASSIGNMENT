# Base class
class Vehicle:
    def move(self):
        return "Moving..."

# Subclasses with overridden move()
class Car(Vehicle):
    def move(self):
        return "Driving ğŸš—"

class Plane(Vehicle):
    def move(self):
        return "Flying âœˆï¸"

class Boat(Vehicle):
    def move(self):
        return "Sailing ğŸš¢"


# Function to demonstrate polymorphism
def travel(vehicle):
    print(vehicle.move())

# Test
my_car = Car()
my_plane = Plane()
my_boat = Boat()

travel(my_car)
travel(my_plane)
travel(my_boat)
